CREATE INDEX `cid_idx` ON `deepln`(`caseId`)
CREATE INDEX `date_idx` ON `deepln`(`date`) WHERE (`type` = "record")
CREATE INDEX `type_idx` ON `deepln`(`type`)
CREATE INDEX `draft_caseId_idx` ON `deepln`(`caseId`) WHERE (`type` = "draft")
CREATE INDEX `draft_username_idx` ON `deepln`(`username`) WHERE (`type` = "draft")
CREATE INDEX `pid_idx` ON `deepln`(`patientId`) WHERE (`type` = "record")
CREATE INDEX `pins_idx` ON `deepln`(`patientId`,`patientName`,`patientSex`) WHERE (`type` = "record")
CREATE INDEX `pname_idx` ON `deepln`(`patientName`) WHERE (`type` = "record")
CREATE INDEX `psex_idx` ON `deepln`(`patientSex`) WHERE (`type` = "record")
CREATE INDEX `rough_date` ON `deepln`((distinct (array `array_element` for `array_element` in suffixes(`date`) end))) WHERE (`type` = "record")
CREATE INDEX `rough_pid` ON `deepln`((distinct (array `array_element` for `array_element` in suffixes(`patientId`) end))) WHERE (`type` = "record")
